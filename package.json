{
  "name": "order-tracker-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "foundation-sites": "^6.4.4-rc1",
    "immutability-helper": "^2.7.0",
    "moment": "^2.22.2",
    "node-sass": "^7.0.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "proxyquire": "^2.0.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-aria-modal": "^2.12.1",
    "react-content-loader": "^3.1.2",
    "react-dom": "^16.3.2",
    "react-ellipsis-text": "^1.0.0",
    "react-foundation": "^0.9.5",
    "react-foundation-components": "^0.14.0",
    "react-ga": "^2.5.3",
    "react-grid-layout": "^0.16.6",
    "react-html-id": "^0.1.2",
    "react-modal": "^3.4.4",
    "react-moment": "^0.7.9",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-scrollable-anchor": "^0.6.1",
    "react-sliding-pane": "^2.0.2",
    "rewire": "^4.0.1",
    "sass-extract": "^2.1.0",
    "sass-extract-loader": "^1.1.0",
    "save": "^2.3.2",
    "save-dev": "^2.0.0",
    "styled-components": "^3.2.6",
    "styled-jsx": "^2.2.6",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "REACT_APP_STAGE=development react-app-rewired start",
    "start-js-test": "REACT_APP_ENV=test react-app-rewired start",
    "build:test": "REACT_APP_ENV=test npm run build",
    "start": "NODE_ENV=development REACT_APP_STAGE=local npm-run-all -p watch-css start-js",
    "start-test": "NODE_ENV=test REACT_APP_ENV=test npm-run-all -p watch-css start-js-test",
    "start-port-80": "PORT=80 NODE_ENV=development REACT_APP_STAGE=local npm-run-all -p watch-css start-js",
    "start-test-api": "NODE_ENV=testapi REACT_APP_ENV=testapi npm-run-all -p watch-css start-js-test",
    "build-js": "react-app-rewired build",
    "build": "NODE_ENV=production REACT_APP_STAGE=production npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom --coverage",
    "test:debug": "react-app-rewired test --inspect-brk test --runInBand --env=jsdom",
    "test:lint:js": "eslint src/**/*.{js,jsx}",
    "test:acceptance:start-app": "npm start --env=test",
    "test:acceptance:local": "env WEBSITE_URL=http://localhost:3000 API=http://localhost:3004/bt-business/v1/ mocha --timeout 10000",
    "test:acceptance:dev": "env DEBUG=nightmare* WEBSITE_URL=https://dev.business.bt.com/order-tracker mocha --timeout 10000",
    "test:acceptance:staging": "env WEBSITE_URL=https://stage.business.bt.com/order-tracker mocha --timeout 10000",
    "test-test": "mocha --compilers js:babel-core/register",
    "test-mocha": "node ./node_modules/mocha/bin/mocha",
    "eject": "react-scripts eject",
    "start-mock-api": "json-server --watch mockAPI/db.json --routes mockAPI/routes.json --port 3004"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "cypress": "^3.0.2",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-matchers": "^6.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "expect": "^23.2.0",
    "expect.js": "^0.3.1",
    "fetch-mock": "^6.5.2",
    "gulp": "^3.9.1",
    "gulp-replace": "^1.0.0",
    "http-proxy-middleware": "^0.18.0",
    "jasmine-node": "^1.14.6",
    "json-server": "^0.14.0",
    "mocha": "^5.2.0",
    "nightmare": "^3.0.1",
    "puppeteer": "^1.5.0",
    "react-app-rewired": "^1.5.2",
    "react-testing-library": "^4.0.2",
    "require-dir": "^1.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "sinon": "^6.0.1",
    "timemachine": "^0.3.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "zombie": "^6.1.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/containers/**/*.js",
      "!src/patterns/**/*.js",
      "!src/layouts/**/*.js",
      "!src/components/Ikons/**/*.js",
      "!src/components/DummyFooter.js",
      "!src/components/DummyHeader.js",
      "!src/components/ContentLoader.js",
      "!src/components/Map.js"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
